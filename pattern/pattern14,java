package pattern;

import java.util.Scanner;

public class pattern14 {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int x = sc.nextInt();
	int n = sc.nextInt();
	for(int i=1;i<=n;i++) {
		int v=n*i;
		System.out.println(x +"*"+i+"="+v);
	}
	System.out.println();

	}

}
